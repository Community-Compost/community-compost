body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF



.navbar
  margin-bottom: 0px;
  padding-left 19px

.navbar a
  text-decoration none
  color black

.navbar-nav > li
  margin: 0px;

.navbar-nav > li > a
  padding-top: 0px

.dropdown-menu > li > a:hover 
  background-color: none 

.nav li a
  margin-top: 10px;

.nav li .button
  margin-top: 0px;

.container
  margin 0

.featurette
  margin-bottom 50px

.first-featurette-left
  margin-bottom 50px

.video-container
    position: relative;
    padding-bottom: 56.25%;
    padding-top: 30px; height: 0; overflow: hidden;
    margin-bottom 50px
 
.video-container iframe, .video-container object, .video-container embed
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;

.partners
  img
    margin-bottom 20px

.registration
  margin-top 20px
  margin-bottom 20px

@media (min-width: 992px)
  .container
    .marketing
      .row
        .featurette
          .first
            margin-top: 25px;

@media (min-width: 768px)
  .profile-title h1
    font-size: 7em

@media (min-width: 850px)
  .navbar .dropdown .dropdown-toggle
    display:none;
  .navbar .dropdown .dropdown-menu
    display:block;

@media (max-width: 850px)
  .navbar .dropdown .dropdown-menu
    margin-right:0;
    margin-top:10px;
